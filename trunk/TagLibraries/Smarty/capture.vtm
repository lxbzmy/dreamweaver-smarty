<tag name="capture" endtag="yes" tagtype="nonempty" casesensitive="yes">
    <tagformat nlbeforetag="1" nlaftertag="1" nlbeforecontents="1" nlaftercontents="1" />
    <attributes>
        <attrib name="name" />
        <attrib name="assign" />
    </attributes>
    <attribcategories>
		<attribgroup name="Required" elements="name,assign" />
	</attribcategories>
</tag>
